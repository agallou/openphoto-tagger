{
    "name": "openphoto-tagger/openphoto-tagger",
    "description": "Rapidly tag your photos on openphoto",
    "license": "MIT",
    "require": {
        "silex/silex": "1.1.x",
        "openphoto-php" : "1.0",
	      "twig/twig" : "1.15.x",
      	"symfony/yaml" : "2.4.x"
    },
    "autoload": {
        "psr-0": { "OpenphotoTagger": "src/" },
        "classmap": [  "vendor/openphoto-php"  ]
    },
    "repositories": [
    {
      "type": "package",
      "package": {
        "name": "openphoto-php",
        "version": "1.0",
        "source": {
          "url": "http://github.com/photo/openphoto-php",
          "type": "git",
          "reference": "master"
        }
      }
    }
    ]
}
